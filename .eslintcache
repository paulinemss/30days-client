rd/index.jsx",[],"/mnt/c/Users/Pauline/Documents/Dev/project3/30days/client/src/pages/CreateCourse.jsx",[],"/mnt/c/Users/Pauline/Documents/Dev/project3/30days/client/src/components/FormDayInput/index.jsx",[],"/mnt/c/Users/Pauline/Documents/Dev/project3/30days/client/src/components/FormDayInput/styles.js",[],"/mnt/c/Users/Pauline/Documents/Dev/project3/30days/client/src/components/Logo/index.jsx",[],"/mnt/c/Users/Pauline/Documents/Dev/project3/30days/client/src/components/CourseForm/index.jsx",[],"/mnt/c/Users/Pauline/Documents/Dev/project3/30days/client/src/pages/EditCourse.jsx",[],"/mnt/c/Users/Pauline/Documents/Dev/project3/30days/client/src/pages/Challenges.jsx",[],"/mnt/c/Users/Pauline/Documents/Dev/project3/30days/client/src/pages/Courses/index.jsx",[],"/mnt/c/Users/Pauline/Documents/Dev/project3/30days/client/src/pages/SinglePages/SingleChallenge.jsx",[],"/mnt/c/Users/Pauline/Documents/Dev/project3/30days/client/src/pages/SinglePages/SingleCourse.jsx",["126","127","128","129","130"],"/* Main imports */ \r\nimport React, { Component } from 'react';\r\nimport { getOneCourse, startChallenge } from '../../services/main';\r\nimport Loading from '../../components/Loading';\r\n\r\n/* Styles imports */\r\nimport { FaHeart } from 'react-icons/fa';\r\nimport { Button, Checkbox, Switch, Modal } from '@material-ui/core';\r\nimport './style.css';\r\n\r\n/* Component */ \r\nexport default class SingleCourse extends Component {\r\n  state = {\r\n    course: {},\r\n    selectedDay: 1,\r\n    currentDetail: '',\r\n    loading: true\r\n  }\r\n\r\n  componentDidMount() {\r\n    getOneCourse(this.props.match.params.id).then(res => {\r\n      this.setState({ \r\n        course: res.data,\r\n        loading: false\r\n      })\r\n    })\r\n  }\r\n\r\n  beginChallenge = () => {\r\n    if (this.props.user) {\r\n      startChallenge(this.state.course, this.props.user._id)\r\n        .then(res => {\r\n          const challenge = res.data;\r\n\r\n          if (!this.props.challenges.includes(challenge)) {\r\n            this.props.addChallenge(challenge);\r\n          } \r\n\r\n          this.props.updateChallenges()\r\n          this.props.history.push(\r\n            `/challenges/page/${challenge.shortId}`\r\n          );\r\n        })\r\n        .catch(err => {\r\n          console.log('err', err);\r\n        })\r\n    } else {\r\n      this.props.history.push(\"/auth/login\");\r\n    }\r\n  }\r\n\r\n  showDayDetails = (num) => {\r\n    this.setState({\r\n      currentDetail: this.state.course.days[num - 1].description\r\n    })\r\n  }\r\n\r\n  render() {\r\n    const { title, smallDescription, longDescription, image, likes, days } = this.state.course; \r\n\r\n    if (this.state.loading) {\r\n      return <Loading />\r\n    }\r\n\r\n    const dailyChallenge = days[this.state.selectedDay - 1]\r\n\r\n    return (\r\n      <div className='single'>\r\n\r\n        <div className='single_side single_left'>\r\n          <div className='single_title'>\r\n            <h1>30 days of... {title}</h1>\r\n            <h2>{smallDescription}</h2>\r\n          </div>\r\n\r\n          <div className='single_media'>\r\n            <img src={image} alt='' />\r\n            <div>\r\n              <h1>Day {this.state.selectedDay}</h1>\r\n              <h4>30 days of {title}</h4>\r\n            </div>\r\n          </div>\r\n\r\n          <div className='single_details'>\r\n            <div className='single_details-top'>\r\n              <div>\r\n                <p>Day {this.state.selectedDay} Challenge</p>\r\n                <h3>{dailyChallenge.title}</h3>\r\n              </div>\r\n            </div>\r\n            <div className='single_details-bottom'>\r\n              <p>{dailyChallenge.description}</p>\r\n              {dailyChallenge.externalUrl && \r\n                <Button>\r\n                  <a\r\n                    href={dailyChallenge.externalUrl}\r\n                    target='_blank'\r\n                    rel='noreferrer'\r\n                  >External link</a>\r\n                </Button>\r\n              }\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div className='single_side single_right'>\r\n          <div className='single_right-top'>\r\n            <h3>About this course</h3>\r\n            <p>{longDescription}</p>\r\n            \r\n          </div>\r\n\r\n          <div className='single_calendar'>\r\n            <div className='single_calendar-title'>\r\n              <p>30 days of {title}</p>\r\n            </div>\r\n            <div className='single_numbers'>\r\n              {days.map(day => (\r\n                <button\r\n                  className={`\r\n                    single_calendar-num\r\n                    ${this.state.selectedDay === day.dayNumber ? 'active' : ''}\r\n                  `}\r\n                  key={day._id}\r\n                >\r\n                  {day.dayNumber}\r\n                </button>\r\n              ))}\r\n            </div>\r\n          </div>\r\n\r\n          <div className='single_streak'>\r\n            <Button\r\n              variant='outlined'\r\n              color='primary'\r\n              onClick={this.beginChallenge}\r\n            >\r\n              Start 30 days challenge\r\n            </Button>\r\n          </div>\r\n\r\n        </div>\r\n\r\n      </div>\r\n    )\r\n  }\r\n}\r\n","/mnt/c/Users/Pauline/Documents/Dev/project3/30days/client/src/pages/NotFound/index.jsx",[],"/mnt/c/Users/Pauline/Documents/Dev/project3/30days/client/src/components/QuitChallengeModal/index.jsx",[],"/mnt/c/Users/Pauline/Documents/Dev/project3/30days/client/src/pages/Settings/index.jsx",[],"/mnt/c/Users/Pauline/Documents/Dev/project3/30days/client/src/components/PageTitle/index.jsx",[],"/mnt/c/Users/Pauline/Documents/Dev/project3/30days/client/src/utils/helpers.js",[],"/mnt/c/Users/Pauline/Documents/Dev/project3/30days/client/src/components/ProgressBar/index.jsx",[],{"ruleId":"131","replacedBy":"132"},{"ruleId":"133","replacedBy":"134"},